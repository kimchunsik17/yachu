        <Label x:Name="Yacht_indi" Content="" HorizontalAlignment="Left" Margin="518,386,0,0" VerticalAlignment="Top" Height="34" Width="169"/>
        <Button x:Name="d1" Content="0" HorizontalAlignment="Left" Margin="163,482,0,0" VerticalAlignment="Top" Height="31" Width="29" FontSize="18" FontFamily="Arial Black" RenderTransformOrigin="0.333,0.754" Click="d1_Click" IsEnabled="False" Opacity="0.5"/>
